{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel-lang/lang": "^15.25",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "nwidart/laravel-modules": "^12.0",
        "prettus/l5-repository": "^3.0",
        "tymon/jwt-auth": "^2.2"
    },
    "require-dev": {
        "darkaonline/l5-swagger": "^10.0",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Core\\": "core/",
            "Modules\\Academic\\App\\": "modules/Academic/App/",
            "Modules\\Academic\\Database\\Factories\\": "modules/Academic/Database/Factories/",
            "Modules\\Academic\\Database\\Seeders\\": "modules/Academic/Database/Seeders/",
            "Modules\\Admin\\App\\": "modules/Admin/App/",
            "Modules\\Admin\\Database\\Factories\\": "modules/Admin/Database/Factories/",
            "Modules\\Admin\\Database\\Seeders\\": "modules/Admin/Database/Seeders/",
            "Modules\\Attendance\\App\\": "modules/Attendance/App/",
            "Modules\\Attendance\\Database\\Factories\\": "modules/Attendance/Database/Factories/",
            "Modules\\Attendance\\Database\\Seeders\\": "modules/Attendance/Database/Seeders/",
            "Modules\\Channel\\App\\": "modules/Channel/App/",
            "Modules\\Channel\\Database\\Factories\\": "modules/Channel/Database/Factories/",
            "Modules\\Channel\\Database\\Seeders\\": "modules/Channel/Database/Seeders/",
            "Modules\\Core\\App\\": "modules/Core/App/",
            "Modules\\Core\\Database\\Factories\\": "modules/Core/Database/Factories/",
            "Modules\\Core\\Database\\Seeders\\": "modules/Core/Database/Seeders/",
            "Modules\\Payment\\App\\": "modules/Payment/App/",
            "Modules\\Payment\\Database\\Factories\\": "modules/Payment/Database/Factories/",
            "Modules\\Payment\\Database\\Seeders\\": "modules/Payment/Database/Seeders/",
            "Modules\\Student\\App\\": "modules/Student/App/",
            "Modules\\Student\\Database\\Factories\\": "modules/Student/Database/Factories/",
            "Modules\\Student\\Database\\Seeders\\": "modules/Student/Database/Seeders/"
        },
        "files": [
            "core/Helpers/http-response-functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}